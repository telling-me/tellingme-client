{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://tellingme.site:8080/api/:path*",
      "has": [
        {
          "type": "host",
          "value": "tellingme.co.kr"
        }
      ]
    },
    {
      "source": "/api/:path*",
      "destination": "https://tellingme.store:8080/api/:path*"
    }
  ]
}
